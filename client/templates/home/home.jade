template(name='Home')
  +welcome
template(name="welcome")
  .pages.navbar-through.toolbar-through
    .page.page-on-center(data-page='index')
      +contenidoDashboard
template(name="contenidoDashboard")
  .page-content.dashboard
    +map
    .typed-box
      span.typed
      span.typed-cursor
    .content-block
      .llamadaRender
        a(href="#")
          img.llamada(src="/img/telefonoVector.svg")
      .chatRender
        a(href="{{pathFor 'chat'}}")
          img.chat(src="/img/chatVector.svg")
    img.choque.shadowed(src="/img/choque.svg")
